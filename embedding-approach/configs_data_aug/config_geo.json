{
    "main": {
        "path": "/data/exp_for_resize_distinct"
    },
    "data": {
        "path": "/data/data/train",
        "split_seed": 123
    },
    "model": {
        "input_width": 300,
        "input_height": 400,
        "embedding_size": 256,
        "model_save_path": "/models",
        "cutoff_classes": true,
        "img_preprocess": "crop"
    },
    "train": {
        "batch_size": 64,
        "learning_rate": 1e-5,
        "nb_epochs": 250,
        "sampler": "ensure_positive",
        "class_sampler_config": {
            "samples_per_class": 25,
            "classes_per_batch": 5
        },
        "use_augmentation": true,
        "train_val_split_overlapping": true,
        "augment_config": {
            "use_erase": false,
            "use_intensity": false,
            "use_geometric": true
        },
        "l2_factor": 1e-5
    },
    "predict": {
        "db_path": "/gorilla-reidentification/embedding-approach/database/bristol",
        "model_path": "/gorilla-reidentification/embedding-approach/models_saved/run1/firm-disco-44-epoch=283-val_loss=0.00.ckpt",
        "img_folder": "/data/train_test/test/",
        "n_neighbors": 5
    },
    "create_db": {
        "image_folder": "/data/data/test",
        "db_path": "/gorilla-reidentification/embedding-approach/database",
        "model_path": "/data/models/wandering-eon-36-epoch=396-val_loss=0.01.ckpt"
    }
}