{
    "main": {
        "experiment": "conf mat cxl",
        "datasets": [],
        "datasets_folder": "/data/k-folds2/"
    },
    "model": {
        "input_width": 224,
        "input_height": 224,
        "embedding_size": 256,
        "model_save_path": "/models",
        "cutoff_classes": true,
        "img_preprocess": "crop",
        "backbone": "vit"
    },
    "train": {
        "dataset": {
            "path": "/data/train_test_non_distinct/cxl_all_0_75/"
        },
        "batch_size": 128,
        "learning_rate": 1e-5,
        "nb_epochs": 600,
        "sampler": "ensure_positive",
        "class_sampler_config": {
            "samples_per_class": 8,
            "classes_per_batch": 20
        },
        "use_augmentation": true,
        "train_val_split_overlapping": true,
        "augment_config": {
            "use_erase": false,
            "use_intensity": true,
            "use_geometric": true
        },
        "l2_factor": null
    },
    "predict": {
        "db_path": "/data/db/",
        "model_path": "/models/cxl_all_0_75_final5",
        "img_folder": "/data/experiments/exp3/cxl_all_0_75_final5/eval",
        "n_neighbors": 5
    },
    "create_db": {
        "image_folder": "/data/experiments/exp3/cxl_all_0_75_final5/database_set",
        "db_path": "/data/db/",
        "model_path": "/models/cxl_all_0_75_final5"
    }
}